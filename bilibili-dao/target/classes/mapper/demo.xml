<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD MAPPER 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.Morgan.bilibili.dao.DemoDao">

    <select id="queryNameById" parameterType="java.lang.Long" resultType="java.util.Map">
        select t.* from t_demo as t where t.id = #{userID}
    </select>
    
    <select id="queryCreateTimeById" parameterType="java.lang.Long" resultType="java.lang.String">
        select create_time from t_demo where id = #{userID}
    </select>

</mapper>